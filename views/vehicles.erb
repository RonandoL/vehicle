<h2>See All of our vehicles below:</h2>

<ul>    <!--  display each vehicle model as a link  -->

  <% @vehicles.each() do |vehicle| %>

    <li><a href="/vehicles/<%= vehicle.id() %>"> <%= vehicle.model() %> </li>

  <% end %>
</ul>


<h1><%= @vehicle.make() %></h1>
